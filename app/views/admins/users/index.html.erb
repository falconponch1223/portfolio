<p class="notice"><%= notice %></p>
<div class="container">
  <div class="row">
    <div class="col-xs-6">
      <section class="customer-index">
        <h2>会員一覧</h2>
      　<table class="table table-striped">
          <thead>
            <tr>
              <th>氏名　</th>
              <th>性別　　　　</th>
              <th>メールアドレス　</th>
              <th>会員登録日　</th>
            </tr>
          </thead>
          <tbody>
            <!-- customer each文挿入 -->
            <% @users.each do |user| %>
              <tr>
                <td>
                  <%= link_to admins_user_path(user.id) do %>
                    <%= user.name %>
                  <% end %>
                </td>
                <td><%= user.gender %></td>
                <td><%= user.email %></td>
                <td><%= user.created_at.strftime("%Y年%m月%d日%H時%M分") %></td>
              </tr>
            <% end %>
          </tbody>
        </table>
      </section>
    </div>
  </div>
</div>
